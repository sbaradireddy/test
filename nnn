WITH LKP_REINSTATE AS (
    SELECT DISTINCT POL_PK
    FROM LKP_REINSTATE_WITH_LAPSE1_last
),
LKP_ADD_VEH AS (
    SELECT DISTINCT POL_PK
    FROM LKP_ADD_VEH_DUPLICATES
),
EXPTRANS1 AS (
    SELECT 
        A.*,
        -- Ensure lookup columns do not have NULL values
        COALESCE(LKP_R.POL_PK, 0) AS POL_PK_REINSTATED_WITH_LAPSE,
        COALESCE(LKP_V.POL_PK, 0) AS POL_PK_ADD_VEH,
        -- Apply the filtering logic
        CASE
            WHEN A.DMV_TRANS_TYPE = 'NB' AND SUBSTR(A.POL_VRSN_TXN_TYP, 1, 2) != 'EN' THEN 'N'
            WHEN A.DMV_TRANS_TYPE = 'RS' AND A.REINST_TYP = 'WITH LAPSE' THEN 'N'
            WHEN A.DMV_TRANS_TYPE = 'CN' AND A.ROW_STAT = 'C' THEN 'N'
            WHEN A.DMV_TRANS_TYPE = 'CN' AND A.ROW_STAT != 'C' 
                 AND COALESCE(LKP_R.POL_PK, 0) > 0 
                 AND COALESCE(LKP_R.POL_PK, 0) > A.POL_PK THEN 'N'
            WHEN A.DMV_TRANS_TYPE = 'RS' AND A.REINST_TYP = 'REINSTATE' 
                 AND A.ROW_STAT = 'C' 
                 AND COALESCE(A.POL_PK_CANCEL, 0) = 0 
                 AND NOT EXISTS (SELECT 1 FROM LKP_REINSTATE WHERE POL_PK = A.POL_PK) THEN 'N'
            WHEN A.DMV_TRANS_TYPE = 'AV' AND A.POL_PK = LKP_V.POL_PK THEN 'N'
            ELSE 'Y'
        END AS FILTER_OUT_IND
    FROM EXP_PRE_FILTER A
    LEFT JOIN LKP_REINSTATE LKP_R
        ON A.POL_PK = LKP_R.POL_PK
    LEFT JOIN LKP_ADD_VEH LKP_V
        ON A.POL_PK = LKP_V.POL_PK
),
FILTRANS AS (
    SELECT *
    FROM EXPTRANS1
    WHERE FILTER_OUT_IND = 'N'
)
SELECT *
FROM FILTRANS
WHERE POL_PK IN (83709650, 83499687, 83499688);
