SELECT 
    POL_PK,
    ROW_STAT AS ORIGINAL_ROW_STAT,
    DMV_TRANS_TYPE,
    POL_VRSN_TXN_TYP,
    REINST_TYP,
    POL_PK_REINSTATED_WITH_LAPSE,
    POL_PK_CANCEL,
    POL_PK_ADD_VEH,
    POL_PK,
    CASE
      WHEN DMV_TRANS_TYPE = 'NB' AND SUBSTR(POL_VRSN_TXN_TYP, 1, 2) != 'EN' THEN 'N'
      WHEN DMV_TRANS_TYPE = 'RS' AND REINST_TYP = 'WITH LAPSE' THEN 'N'
      WHEN DMV_TRANS_TYPE = 'CN' AND ROW_STAT = 'C' THEN 'N'
      WHEN DMV_TRANS_TYPE = 'CN' AND ROW_STAT = 'H' THEN 'Y'
      ELSE 'Y'
    END AS FILTER_OUT_IND,
    CASE
      WHEN DMV_TRANS_TYPE = 'CN' AND ROW_STAT = 'H' THEN 'H'
      WHEN DMV_TRANS_TYPE = 'CN' AND ROW_STAT = 'C' THEN 'C'
      ELSE ROW_STAT
    END AS FINAL_ROW_STAT
FROM EXP_PRE_FILTER
WHERE POL_PK = 234;
