AGG_NJ_DMV_MTHLY_MERGE_HEADER_FF AS (
    SELECT 
        source_record_id,
        DMV_INS_COMPANY_CD,
        DMV_TIME_STAMP,
        DMV_DATE_STAMP,
        BATCH_END_DT,
        
        -- Construct HEADER field by concatenating relevant fields
        RPAD('HEADER', 6, ' ') || DMV_INS_COMPANY_CD || DMV_TIME_STAMP || DMV_DATE_STAMP || 
        LPAD(BATCH_END_DT, 8, '0') AS HEADER
    FROM EXP_NJ_DMV_MTHLY_MERGE_HEADER_FF
    GROUP BY source_record_id, DMV_INS_COMPANY_CD, DMV_TIME_STAMP, DMV_DATE_STAMP, BATCH_END_DT
)
