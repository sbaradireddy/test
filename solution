		DDCTBL_1_AMT,
		DDCTBL_2_DESC,
		DDCTBL_2_AMT,
		BUS_PPTY_INCRSD_LMT,
		LMT_BCL,
		LMT_BOAT,
		LMT_BOAT_EQPT,
		LMT_BOAT_MOTR,
		LMT_BOAT_TRLR,
		LMT_CAMERA,
		LMT_COINS,
		LMT_ELTRNCS,
		LMT_ELTRNCS_OFF_PRMSES,
		LMT_ELTRNCS_ON_PRMSES,
		LMT_ELTRNCS_IN_VEH,
		LMT_ELTRNCS_OUT_VEH,
		LMT_FINE_ART,
		LMT_FINE_ART_EXCLDG_BRKAGE,
		LMT_FINE_ART_INCLG_BRKAGE,
		LMT_FINE_ART_DRBL,
		LMT_FINE_ART_FRAGILE,
		LMT_FINE_ART_PPR,
		LMT_FURS,
		LMT_GOLF,
		LMT_GOLF_CART,
		LMT_GUNS,
		LMT_JEWELERY,
		LMT_JEWELERY_IN_VALT,
		LMT_JEWELERY_NO_VALT,
		LMT_MONEY,
		LMT_MUSICL_INSTMT,
		LMT_MUSICL_INSTMT_NON_PFSNL,
		LMT_MUSICL_INSTMT_PFSNL,
		LMT_OTHR_STRCT_1,
		LMT_OTHR_STRCT_2,
		LMT_OTHR_STRCT_3,
		LMT_OTHR_STRCT_4,
		LMT_BLDG_ADDNS,
		LMT_SCRTYS,
		LMT_SLVRW,
		LMT_STAMPS,
		CVG_PK,
		ROW_NUMBER() OVER(PARTITION BY CVG_PK
	ORDER BY
		CVG_PK ) AS rn
		-- Please review the order by columns 
	FROM
		V_POL_CVG_FACT VPCF
	WHERE
		VPCF.Prcsg_Grp_Cd = 'NE'
		AND
VPCF.Co_Cd IN ('BHIC', 'MWAC', 'PRAC', 'BHICC', 'BHPIC', 'BHSIC', 'BHPFIC')
			AND
VPCF.Prod_Cd IN ('HO', 'PCAT', 'DF')
				AND
VPCF.Corp_Cd = 'PRC'
QUALIFY rn = 1 ) LKP_T_POL_CVG_FACT_LIMITS
ON
	LKP_T_POL_CVG_FACT_LIMITS.CVG_PK = SQ_S10_ISO_BURRPT_PLOTA_PREM_EXTRACT.ORIG_CVG_PK 
)
--select * from SQ_S10_ISO_BURRPT_PLOTA_PREM_EXTRACT limit 5
,
EXPTRANS2 AS (
-- writting query for expression function
SELECT
	SQ_S10_ISO_BURRPT_PLOTA_PREM_EXTRACT.source_record_id,
	SQ_S10_ISO_BURRPT_PLOTA_PREM_EXTRACT.POL_PK,
	SQ_S10_ISO_BURRPT_PLOTA_PREM_EXTRACT.CUR_TERM_EFF_DT,
	DATEADD('YYYY',
	- 3,
	SQ_S10_ISO_BURRPT_PLOTA_PREM_EXTRACT.CUR_TERM_EFF_DT) AS EARLIEST_LOSS_DATE,
	LKP_LOSS_HISTORY_COUNT.INCDT_COUNT
FROM
	SQ_S10_ISO_BURRPT_PLOTA_PREM_EXTRACT SQ_S10_ISO_BURRPT_PLOTA_PREM_EXTRACT
LEFT JOIN
	-- writting query for lookup function
 (
	SELECT
		VPIDH.POL_PK AS POL_PK,
		COUNT(VPIDH.INCDT_PK) AS INCDT_COUNT,
		ROW_NUMBER() OVER(PARTITION BY VPIDH.POL_PK
	ORDER BY
		VPIDH.POL_PK) AS rn
		-- Please review the order by columns
	FROM
		V_POL_INCDT_DIM_HST VPIDH,
		V_POL_DIM_HST_CORE VPDHC
	WHERE
		VPIDH.Prcsg_Grp_Cd = 'NE'
		AND VPIDH.Co_Cd IN ('BHIC', 'BHICC', 'MWAC', 'PRAC', 'BHPIC', 'BHSIC', 'BHPFIC')
			AND VPIDH.Prod_Cd IN ('DF', 'HO', 'PCAT')
				AND VPIDH.Corp_Cd = 'PRC'
				AND VPIDH.Rcd_Actn_Typ <> 'D'
				AND VPIDH.Incdt_Typ_Cd = 'CLAIMS'
				AND VPIDH.Rcd_Actn_Typ <> 'D'
				AND VPIDH.Clm_Loss_Typ_Cd NOT IN ('HAIL', 'MEDIC', 'QUAKE', 'SINK', 'WIND')
					AND VPIDH.Clm_Net_Paid_Amt > 499.99
					AND VPIDH.INCDT_DT < VPDHC.Cur_Term_Eff_Dt
					AND VPIDH.INCDT_DT >= (DATEADD(YEARS,
					-3,
					VPDHC.Cur_Term_Eff_Dt))
						AND VPDHC.Pol_Pk = VPIDH.Pol_Pk
						--qualify rn = 1
					GROUP BY
						VPIDH.POL_PK
               ) LKP_LOSS_HISTORY_COUNT
            ON
	LKP_LOSS_HISTORY_COUNT.POL_PK = SQ_S10_ISO_BURRPT_PLOTA_PREM_EXTRACT.POL_PK
       )
--select * from EXPTRANS2
       ,
      
