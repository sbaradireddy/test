	VPPUDH.AT_FLT_FLG,
	VPPUDH.CLERICAL_EMPLOYEE_FLG,
	VPPUDH.INCDNTL_OCC_FLG,
	VPPUDH.HM_OFFICE_FLG,
	VPPUDH.POOL_FLG,
	VPPUDH.HM_SRVCS_FLG,
	VPPUDH.SPA_FLG,
	VPPUDH.TCHR_FLG,
	VPPUDH.TRUST_FLG,
	VPPUDH.UNDLYG_AUTO_POL_LMT,
	VPPUDH.UNDLYG_PPTY_POL_LMT,
	VPPUDH.PI_LIAB_CVG_FLG,
	VPPUDH.YTHFL_OPER_SRCHG_CT,
	VPLUDH.PSTL_CD,
	VPCDDH.BOAT_USG,
	VPCDDH.BOAT_EQPT,
	VPLUDH.BCEG,
	VPLUDH.ROOF_AGE,
	VPLUDH.YR_ROOF_INST,
	VPLUDH.POOL_CD,
	VPLUDH.TRMP_FLG
FROM
	V_POL_CVG_FACT VPCF ,
	V_PROD_DIM VPD,
	V_CO_DIM VCD,
	V_POL_STAT_DIM_HST VPSDH
LEFT OUTER JOIN V_POL_DIM_HST_CORE VPDHC ON
	(VPSDH.ORIG_POL_PK = VPDHC.POL_PK) ,
	V_POL_STAT_PREM_FACT VSPF
LEFT OUTER JOIN V_POL_CVG_DTLS_DIM_HST VPCDDH ON
	(VSPF.ORIG_CVG_PK = VPCDDH.CVG_PK) ,
	V_POL_STAT_UNIT_DIM_HST VPSUDH
LEFT OUTER JOIN V_POL_LCTN_UNIT_DIM_HST VPLUDH ON
	(VPSUDH.ORIG_LCTN_PK = VPLUDH.LCTN_PK)
LEFT OUTER JOIN V_POL_PCAT_UNIT_DIM_HST VPPUDH ON
	(VPSUDH.ORIG_PCAT_UNIT_PK = VPPUDH.PCAT_UNIT_PK)
WHERE
	(VPCF.CVG_PK = VSPF.CVG_PK
		AND
  VSPF.POL_STAT_UNIT_PK = VPSUDH.POL_STAT_UNIT_PK
		AND
  VPSUDH.POL_STAT_PK = VPSDH.POL_STAT_PK
		AND
  VPCF.CO_ID = VCD.CO_ID
		AND VPCF.PROD_ID = VPD.PROD_ID)
	AND
 (VPCF.PRCSG_GRP_CD = 'NE'
		AND
 RTRIM(LTRIM(VPCF.CO_CD)) IN ('BHIC', 'MWAC', 'PRAC', 'BHICC', 'BHPIC', 'BHSIC', 'BHPFIC')
			AND
 RTRIM(LTRIM(VPCF.PROD_CD)) IN ('HO', 'PCAT', 'DF')
				AND
 RTRIM(LTRIM(VPCF.CORP_CD)) = 'PRC'
					AND
					--TRY_TO_DATE(VPCF.ACCTG_DT,'MMDDYYYY') <= '2024/31/09' AND
					--TRY_TO_DATE(VPCF.ACCTG_DT,'MMDDYYYY') >= '2024/01/09' AND
					RTRIM(LTRIM(VSPF.PREMIUM_SOURCE_T)) <> 'MISC'
						AND
 VSPF.CNTRB_TO_PREMIUM_F = 'Y'
						AND
 VSPF.RCD_ACTN_TYP <> 'D')
ORDER BY
	VSPF.SRC_POL_ID,
	VSPF.SRC_RISK_UNIT_ID,
	VSPF.POL_STATE_VRSN,
	VPSDH.SRC_DATA_SET_NBR,
	VSPF.PREMIUM_SOURCE_C
 )
--select * from SQ_S10_ISO_BURRPT_PLOTA_PREM_EXTRACT_temp
