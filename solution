LEFT JOIN
	-- writting query for lookup function 
  (
	SELECT
		LMT_1_DESC,
		LMT_1_AMT,
		LMT_2_DESC,
		LMT_2_AMT,
		LMT_3_DESC,
		LMT_3_AMT,
