UNI_NJ_DMV_GENERIC_MTHLY_MERGE_DATA_FF AS (
    SELECT 
        HEADER AS INPUT_RECORD  -- Ensure this column exists in AGG_NJ_DMV_MTHLY_MERGE_HEADER_FF
    FROM AGG_NJ_DMV_MTHLY_MERGE_HEADER_FF
    
    UNION ALL

    SELECT 
        DETAIL AS INPUT_RECORD  -- Ensure this column exists in EXP_EXP_NJ_DMV_MTHLY_MERGE_DETAIL_FF
    FROM EXP_EXP_NJ_DMV_MTHLY_MERGE_DETAIL_FF
    
    UNION ALL

    SELECT 
        TRAILER AS INPUT_RECORD  -- Ensure this column exists in EXP_TRAILER
    FROM EXP_TRAILER
)
