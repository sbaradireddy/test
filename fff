               S10_NJ_DMV_MTHLY_EXTRACT.DMV_RESERVED
          FROM S10_NJ_DMV_MTHLY_EXTRACT),
LKP_ADD_VEH_DUPLICATES_cache as 
 (
                select POL_PK,
                       POL_NUM,
                       DMV_TRANS_TYPE,
                       POL_STATE_EFF_DT,
                       DMV_VIN,
                       row_number() over(partition by POL_NUM, DMV_TRANS_TYPE /*, POL_STATE_EFF_DT,*/  ,DMV_VIN order by POL_NUM, DMV_TRANS_TYPE, POL_STATE_EFF_DT, DMV_VIN desc) as rn -- Please review the order by columns

                  FROM S10_NJ_DMV_MTHLY_EXTRACT
                 where DMV_TRANS_TYPE = 'AV'
                 ORDER BY POL_NUM, DMV_TRANS_TYPE, POL_STATE_EFF_DT, DMV_VIN
               ),              
LKP_ADD_VEH_DUPLICATES_any as 
(select S10_NJ_DMV_MTHLY_EXTRACT.POL_NUM,S10_NJ_DMV_MTHLY_EXTRACT.DMV_TRANS_TYPE ,S10_NJ_DMV_MTHLY_EXTRACT.POL_STATE_EFF_DT,LKP_ADD_VEH_DUPLICATES.POL_PK as lkp_POL_PK,
S10_NJ_DMV_MTHLY_EXTRACT.DMV_VIN, max(rn) as rn from LKP_ADD_VEH_DUPLICATES_cache LKP_ADD_VEH_DUPLICATES join 
S10_NJ_DMV_MTHLY_EXTRACT S10_NJ_DMV_MTHLY_EXTRACT 
on LKP_ADD_VEH_DUPLICATES.POL_NUM=S10_NJ_DMV_MTHLY_EXTRACT.POL_NUM
           AND LKP_ADD_VEH_DUPLICATES.DMV_TRANS_TYPE=S10_NJ_DMV_MTHLY_EXTRACT.DMV_TRANS_TYPE
           AND LKP_ADD_VEH_DUPLICATES.POL_STATE_EFF_DT<=S10_NJ_DMV_MTHLY_EXTRACT.POL_STATE_EFF_DT
           AND LKP_ADD_VEH_DUPLICATES.DMV_VIN=S10_NJ_DMV_MTHLY_EXTRACT.DMV_VIN
           group by S10_NJ_DMV_MTHLY_EXTRACT.POL_NUM,S10_NJ_DMV_MTHLY_EXTRACT.DMV_TRANS_TYPE ,S10_NJ_DMV_MTHLY_EXTRACT.POL_STATE_EFF_DT,
S10_NJ_DMV_MTHLY_EXTRACT.DMV_VIN,LKP_ADD_VEH_DUPLICATES.POL_PK
),
LKP_REINSTATE_WITH_LAPSE1_cache as 
(
 select POL_PK,
                       SRC_POL_ID,
                       POL_NUM,
                       POL_SEQ_NUM,
                       CO_CD,
                       CUR_TERM_EFF_DT,
                       row_number() over(partition by SRC_POL_ID, POL_PK, POL_NUM, POL_SEQ_NUM, CO_CD, CUR_TERM_EFF_DT order by SRC_POL_ID, POL_PK, POL_NUM, POL_SEQ_NUM, CO_CD, CUR_TERM_EFF_DT) as rn -- Please review the order by columns

                  FROM S10_NJ_DMV_MTHLY_EXTRACT
                 where SUBSTR(POL_VRSN_TXN_TYP,1,2) = 'CN'
                   AND CNCL_RSN IN ('NONPAY','NP','NP_CR','NP_PF')
                   AND ROW_STAT <> 'C'
                   group by SRC_POL_ID, POL_PK, POL_NUM, POL_SEQ_NUM, CO_CD, CUR_TERM_EFF_DT
),
LKP_REINSTATE_WITH_LAPSE1_last as 
(
select   S10_NJ_DMV_MTHLY_EXTRACT.POL_PK,
S10_NJ_DMV_MTHLY_EXTRACT.SRC_POL_ID,
S10_NJ_DMV_MTHLY_EXTRACT.POL_NUM,
S10_NJ_DMV_MTHLY_EXTRACT.POL_SEQ_NUM,
S10_NJ_DMV_MTHLY_EXTRACT.CO_CD,
LKP_REINSTATE_WITH_LAPSE1.POL_PK as LKP_REINSTATE_WITH_LAPSE1_lkp_POL_PK,
S10_NJ_DMV_MTHLY_EXTRACT.CUR_TERM_EFF_DT, max(rn) as rn
from LKP_REINSTATE_WITH_LAPSE1_cache LKP_REINSTATE_WITH_LAPSE1 join S10_NJ_DMV_MTHLY_EXTRACT S10_NJ_DMV_MTHLY_EXTRACT
 on LKP_REINSTATE_WITH_LAPSE1.SRC_POL_ID=S10_NJ_DMV_MTHLY_EXTRACT.SRC_POL_ID
           AND LKP_REINSTATE_WITH_LAPSE1.POL_PK<S10_NJ_DMV_MTHLY_EXTRACT.POL_PK
           AND LKP_REINSTATE_WITH_LAPSE1.POL_NUM=S10_NJ_DMV_MTHLY_EXTRACT.POL_NUM
           AND LKP_REINSTATE_WITH_LAPSE1.POL_SEQ_NUM=S10_NJ_DMV_MTHLY_EXTRACT.POL_SEQ_NUM
           AND LKP_REINSTATE_WITH_LAPSE1.CO_CD=S10_NJ_DMV_MTHLY_EXTRACT.CO_CD
           AND LKP_REINSTATE_WITH_LAPSE1.CUR_TERM_EFF_DT=S10_NJ_DMV_MTHLY_EXTRACT.CUR_TERM_EFF_DT
group by S10_NJ_DMV_MTHLY_EXTRACT.POL_PK,
S10_NJ_DMV_MTHLY_EXTRACT.SRC_POL_ID,
S10_NJ_DMV_MTHLY_EXTRACT.POL_NUM,
S10_NJ_DMV_MTHLY_EXTRACT.POL_SEQ_NUM,
S10_NJ_DMV_MTHLY_EXTRACT.CO_CD,
S10_NJ_DMV_MTHLY_EXTRACT.CUR_TERM_EFF_DT,
LKP_REINSTATE_WITH_LAPSE1.POL_PK
),
LKP_CANCEL_cache as 
(
                select POL_PK,
                       SRC_POL_ID,
                       POL_NUM,
                       POL_SEQ_NUM,
                       CO_CD,
                       CUR_TERM_EFF_DT,
                       row_number() over(partition by SRC_POL_ID, POL_PK, POL_NUM, POL_SEQ_NUM, CO_CD, CUR_TERM_EFF_DT order by SRC_POL_ID, POL_PK, POL_NUM, POL_SEQ_NUM, CO_CD, CUR_TERM_EFF_DT asc) as rn -- Please review the order by columns

                  FROM S10_NJ_DMV_MTHLY_EXTRACT
                 where SUBSTR(POL_VRSN_TXN_TYP,1,2) = 'RS'
                   AND REINST_TYP = 'WITH LAPSE'
                   AND CNCL_RSN IN ('NONPAY','NP','NP_CR','NP_PF')
                   group by POL_PK,
                       SRC_POL_ID,
                       POL_NUM,
                       POL_SEQ_NUM,
                       CO_CD,
                       CUR_TERM_EFF_DT
),
LKP_CANCEL_any as 
(
 select                S10_NJ_DMV_MTHLY_EXTRACT.POL_PK,
                       S10_NJ_DMV_MTHLY_EXTRACT.SRC_POL_ID,
                       S10_NJ_DMV_MTHLY_EXTRACT.POL_NUM,
                       S10_NJ_DMV_MTHLY_EXTRACT.POL_SEQ_NUM,
                       S10_NJ_DMV_MTHLY_EXTRACT.CO_CD,
                       S10_NJ_DMV_MTHLY_EXTRACT.CUR_TERM_EFF_DT,
                       LKP_CANCEL.POL_PK as LKP_CANCEL_lkp_POL_PK,
                       max(rn) as rn
from LKP_CANCEL_cache LKP_CANCEL join S10_NJ_DMV_MTHLY_EXTRACT S10_NJ_DMV_MTHLY_EXTRACT 
on LKP_CANCEL.SRC_POL_ID=S10_NJ_DMV_MTHLY_EXTRACT.SRC_POL_ID
           AND LKP_CANCEL.POL_PK>S10_NJ_DMV_MTHLY_EXTRACT.POL_PK
           AND LKP_CANCEL.POL_NUM=S10_NJ_DMV_MTHLY_EXTRACT.POL_NUM
           AND LKP_CANCEL.POL_SEQ_NUM=S10_NJ_DMV_MTHLY_EXTRACT.POL_SEQ_NUM
           AND LKP_CANCEL.CO_CD=S10_NJ_DMV_MTHLY_EXTRACT.CO_CD
           AND LKP_CANCEL.CUR_TERM_EFF_DT=S10_NJ_DMV_MTHLY_EXTRACT.CUR_TERM_EFF_DT
           group by S10_NJ_DMV_MTHLY_EXTRACT.POL_PK,
                       S10_NJ_DMV_MTHLY_EXTRACT.SRC_POL_ID,
                       S10_NJ_DMV_MTHLY_EXTRACT.POL_NUM,
                       S10_NJ_DMV_MTHLY_EXTRACT.POL_SEQ_NUM,
                       S10_NJ_DMV_MTHLY_EXTRACT.CO_CD,
                       S10_NJ_DMV_MTHLY_EXTRACT.CUR_TERM_EFF_DT,
                       LKP_CANCEL.POL_PK 
),
